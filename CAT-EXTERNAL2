 if IG == 10 then MAS10() end
 if IG == 11 then Exit() end
 menuk = -1
 end


--------Function 1

function MAS1()
gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber('8.40779079e-45F;0D;0F;30F;50F;0.5F::33', gg.TYPE_DWORD)
    gg.refineNumber('0', gg.TYPE_DWORD)
    gg.getResults(1)-----[[© Dec By メㅤJᴜʟʟㅤ4Yᴏᴜ 亗 ]]-----
    gg.editAll('-1', gg.TYPE_DWORD)
    gg.clearResults()
    gg.toast('Aim awm')
  end


--------Function 2
function MAS2()

gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_CODE_APP)
    gg.searchNumber('h 00 00 00 00 3f 00 00 80 3e', gg.TYPE_BYTE)
    gg.getResults(100)-----[[© Dec By メㅤJᴜʟʟㅤ4Yᴏᴜ 亗 ]]-----
    gg.editAll('h 00 ec 51 b8 3d 8f c2 f5 3c', gg.TYPE_BYTE)
    gg.clearResults()
    gg.toast('Fast change Sniper')
  end


--------Function 3
function MAS3()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.setVisible(false)
gg.searchNumber("7209065;6357095;6619245;6357039;7536755;7536745;6357108;7602286;7602281;7143525;6881327;6619235;6357111;7077996;6422623;7209077;6619243;114:69", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
--[[ found: 18 ]]
gg.getResults(99999, nil, nil, nil, nil, nil, nil, nil, nil)
--[[ count: 18 ]]
gg.editAll(";effects/vfx_pet/vfx_petskill_robot", gg.TYPE_DWORD)
gg.processResume()
gg.clearResults()
  end

--------Function 4
function MAS4()

gg.clearResults()
    gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber("909,000,000~909000160", gg.TYPE_DWORD)
    gg.getResults(1000)
    gg.editAll("909,000,014", gg.TYPE_DWORD)
    gg.clearResults()
    gg.toast("✅ᴛʜʀᴏɴᴇ ᴇᴍᴏᴛᴇ ᴏɴ✅")
  end

