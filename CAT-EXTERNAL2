end



--------Function 11 (EXIT)
function Exit()
gg.alert("»» ------------------------------------------ «« \n\n CAT-EXTERNAL\n\n»» ------------------------------------------ ««")
print(" \n ♻️ YOUTUBE : CAT-EXTERNAL  ")
gg.setVisible(true)
os.exit()
end


while true do
  if gg.isVisible(true) then
    menuk = 1
    gg.setVisible(false)
  end
  if menuk == 1 then
    START()
  end
  if menu == 2 then
    START()
    end
  menuk = -1
end
